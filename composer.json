{
    "name": "beartropy/charts",
    "description": "Chart components for the Beartropy ecosystem.",
    "version": "1.0.3",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Beartropy\\Charts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beartropy\\Charts\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beartropy\\Charts\\ChartsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
